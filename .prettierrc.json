{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "singleQuote": false,
  "printWidth": 80,
  "vueIndentScriptAndStyle": false,
  "singleAttributePerLine": true,
  "trailingComma": "all",
  "bracketSameLine": false,
  "tabWidth": 2,
  "arrowParens": "always",
  "bracketSpacing": true,
  "objectWrap": "preserve",
  "endOfLine": "lf"
}
